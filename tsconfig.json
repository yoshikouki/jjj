{
	"extends": "@sindresorhus/tsconfig",
	"compilerOptions": {
		"outDir": "dist",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "Node16",
		"noPropertyAccessFromIndexSignature": false,
		"jsx": "react-jsx"
	},
	"include": ["src"],
	"ts-node": {
		"esm": true
	}
}
